{
  "name": "bfast-database-core",
  "version": "3.0.0-alpha.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "es2015": "dist/es2015/index.js",
  "esm2015": "dist/esm/index.js",
  "license": "MIT",
  "author": {
    "name": "FahamuTech",
    "email": "mama27j@gmail.com",
    "url": "https://fahamutech.com"
  },
  "nyc": {
    "include": "dist",
    "exclude": "**/*.spec.js"
  },
  "maintainers": [
    "joshuamshana(https://github.com/joshuamshana)",
    "fahamutech(https://github.com/fahamutech)"
  ],
  "scripts": {
    "build": "gulp build && yarn build:cjs && yarn build:es2015 && yarn build:esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/",
    "test": "yarn run build && nyc mocha  specs/src/*",
    "prestart": "yarn run build",
    "prepublishOnly": "npm run build",
    "publish:beta": "yarn publish --tag beta"
  },
  "dependencies": {
    "base64url": "~3.0.1",
    "bcryptjs": "~2.4.3",
    "bfast": "~5.4.2",
    "bfast-database-tree": "~1.1.4",
    "formidable": "~1.2.2",
    "http-status-codes": "~2.1.4",
    "ipfs-http-client": "~53.0.1",
    "it-to-stream": "~1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "~2.5.2",
    "minio": "~7.0.19",
    "moment": "^2.29.1",
    "mongodb": "~4.1.2",
    "sharp": "~0.29.1",
    "uuid": "~8.3.2",
    "web3.storage": "~3.3.2"
  },
  "devDependencies": {
    "@types/express": "~4.17.13",
    "@types/minio": "~7.0.8",
    "@types/mongodb": "~3.6.20",
    "@types/node": "~14.14.10",
    "@types/sharp": "~0.28.4",
    "axios": "~0.22.0",
    "bfast-function": "~1.13.5",
    "chai": "^4.3.4",
    "del": "~6.0.0",
    "engine.io-client": "~5.2.0",
    "form-data": "^4.0.0",
    "gulp": "~4.0.2",
    "ipfs-only-hash": "^4.0.0",
    "mocha": "~9.1.2",
    "nyc": "~15.1.0",
    "typescript": "~4.4.3"
  }
}
