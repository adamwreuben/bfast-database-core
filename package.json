  "scripts": {
    "build": "gulp build",
    "build:docker": "gulp buildDocker",
    "test:web-services": "yarn run build && mocha --exit specs/rest/all.specs.js",
    "test:controllers": "yarn run build && npx mocha --exit specs/controller/* ",
