    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "minio": "^7.0.17",
    "mongodb": "^3.6.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/minio": "^7.0.6",
    "@types/mongodb": "^3.5.34",
    "@types/node": "^14.14.10",
    "axios": "^0.21.0",
    "del": "^6.0.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
